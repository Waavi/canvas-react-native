{
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"root/*": ["./*"],
			"@/*": ["./src/*"],
			"@modules/*": ["./modules/*"],
			"#actions": ["./src/store/actions"],
			"#selectors": ["./src/store/selectors"],
			"#flags": ["./src/store/flags/flags"],
			"#propTypes": ["./src/utils/propTypes"]
		},
		"experimentalDecorators": true,
		"allowSyntheticDefaultImports": true
	},
	"exclude": ["node_modules"]
}
